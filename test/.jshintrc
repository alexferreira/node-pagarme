{
    "eqnull": true,
    "strict": true,
    "expr": true,
    "noarg": true,
    "node": true,
    "trailing": true,
    "unused": true,
    "laxbreak": true,
    "laxcomma": true,
    "predef": [
      "-Promise"
    ],
    "globals": {
      /* MOCHA */
      "describe"   : false,
      "it"         : false,
      "before"     : false,
      "beforeEach" : false,
      "after"      : false,
      "afterEach"  : false
  }
}
